<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PublishAot>false</PublishAot>
    <InvariantGlobalization>true</InvariantGlobalization>
	<EmbedAllSources>true</EmbedAllSources>

	<Title>RailGrinder</Title>
	<Description>Leaderboard Analyzer for Synthriderz</Description>
	<AssemblyVersion>1.2.4.1</AssemblyVersion>
	<FileVersion>1.2.4.1</FileVersion>
	<InformationalVersion>1.2.4.1</InformationalVersion>
	<ContinuousIntegrationBuild>false</ContinuousIntegrationBuild>
	<GenerateAssemblyInformationalVersionAttribute>false</GenerateAssemblyInformationalVersionAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
